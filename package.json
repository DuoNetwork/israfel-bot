{
	"name": "israfel-bot",
	"version": "1.0.0",
	"private": true,
	"description": "Israfel Bot",
	"author": "FinBook Pte Ltd",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"makeDepth": "ts-node ./src/index makeDepth",
		"allowance": "ts-node ./src/index allowance"
	},
	"jest": {
		"testEnvironment": "node",
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"devDependencies": {
		"@types/jest": "^23.3.10",
		"@types/ws": "^6.0.1",
		"jest": "^23.6.0",
		"pre-commit": "^1.2.2",
		"ts-jest": "^23.10.5",
		"tslint": "^5.11.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.0.0",
		"moment": "^2.22.2",
		"ts-node": "^7.0.1",
		"typescript": "^3.1.6",
		"ws": "^6.1.2"
	}
}
